<%= form_for @assignment do |f| %>
    <div class="form-group">
      <%= f.label :name, "Name" %>
      <%= f.text_field :name %>
      <%= f.label :street, "Street Address" %>
      <%= f.text_field :street %>
      <%= f.label :city, "City" %>
      <%= f.text_field :city %>
      <%= f.label :state, "State" %>
      <%= f.text_field :state %>
      <%= f.label :phone, "Phone Number" %>
      <%= f.text_field :phone %>
    </div>
  
  
  
    <div class="actions">
      <%= f.submit "Create", class: "btn btn-primary" %>
    </div>
  
  <% end  %>